#!/bin/sh
APPDIR=`dirname $0`;
VENDOR_LIBS="$APPDIR/lib/gwt-rest.jar:$APPDIR/lib/gwtx.jar" # append custom entries here
GWT_ROOT="<%= gwt_home %>"
PORT="3000"
java -XstartOnFirstThread -cp "$APPDIR/src:$VENDOR_LIBS:$APPDIR/bin:$GWT_ROOT/gwt-user.jar:$GWT_ROOT/gwt-dev-<%= gwt_dev_platform %>.jar" com.google.gwt.dev.GWTShell -out "$APPDIR/../public/gwt" "$@" -noserver -port $PORT ui;
