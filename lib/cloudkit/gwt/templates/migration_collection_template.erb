package ui.client.migration;

import com.kaboomerang.gwt.cloudkit.client.migration.MigrationCollection;
import com.kaboomerang.gwt.cloudkit.client.migration.AbstractMigrationCollection;
import com.kaboomerang.gwt.cloudkit.client.migration.Migration;
import com.kaboomerang.gwt.cloudkit.client.migration.AbstractMigration;

public class ApplicationMigration extends AbstractMigrationCollection implements MigrationCollection {
    public ApplicationMigration() {
        migrations = new Migration[] {
<%- migrations.size.times do |n|-%>
            new Migration<%= n+1 %>()<%- if n != migrations.size-1 -%>,<% end %>
<%- end -%>        };
    }
}