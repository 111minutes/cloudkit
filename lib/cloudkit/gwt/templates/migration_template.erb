package ui.client.migration;

import com.kaboomerang.gwt.cloudkit.client.migration.Migration;
import com.kaboomerang.gwt.cloudkit.client.migration.AbstractMigration;

public class Migration<%= migration_count+1 %> extends AbstractMigration implements Migration {
    public void init(int direction) {
        statements = new String[] {
<%- migrations[migration_count].each do |sql| -%>
            "<%= sql.gsub('"', '\"') -%>"<%- if sql != migrations[migration_count].last -%>,<% end %>
<%- end -%>        };
    }

}